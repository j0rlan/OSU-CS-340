<div class="container">
   <h3>Mediums</h3>
</div>
<div class="container">
   <form action="/mediums" method="post" id="addMedium">
      <fieldset>
         <legend>Add Medium</legend>
         <div>
         <input type="text" name="name" placeholder="Medium" id="name">
         </div>
         <div>
         <textarea form="addMedium" name="description" placeholder="Description of Medium" id="description"></textarea>
         </div>
         <input type="submit" name="add" value="add medium" id="addButton">
      </fieldset>
   </form>
</div>
<div class="container" id="mediumList">
<table>
	<thead>
		<th>Medium</th>
		<th>Description</th>
	</thead>
	<tbody>
		{{#each mediums}}
		<tr>
			<td>{{this.medium_name}}</td>
			<td>{{this.description}}</td>
			<td><button>Delete</button></td>
			<td><button>Update</button></td>
		</tr>
		{{/each}}
	</tbody>
</table>
</div>
<script src="mediumScript.js"></script>
